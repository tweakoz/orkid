fragment_interface iface_fgrid_gbuffer : ub_frg {
  inputs {
    vec3 frg_pos;
    vec4 frg_clr;
    vec2 frg_uv0;
  }
  outputs {
    layout(location = 0) uvec4 out_gbuf;
  }
}
///////////////////////////////////////////////////////////////
fragment_interface iface_grid_forward_unlit {
  inputs {
    vec2 frg_uv0;
    vec3 frg_pos;
  }
  outputs {
    layout(location = 0) vec4 out_clr;
  }
}
fragment_interface iface_grid_forward_lit {
  inputs {
    vec2 frg_uv0;
    vec4 frg_wpos;
    mat3 frg_tbn;
    vec4 frg_clr;
    float frg_camdist;
    vec3 frg_camz;
  }
  outputs {
    layout(location = 0) vec4 out_clr;
  }
}
