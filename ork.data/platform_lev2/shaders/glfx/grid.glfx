vertex_shader vs_rigid_gbuffer_grid : iface_vtx_fwd_grid_unlit {
  frg_uv0     = uv0;
  frg_pos     = position.xyz;
  frg_clr     = vec4(1, 1, 1, 1);
  gl_Position = mvp * position;
}
libblock lib_frgdeferred_grid_pbr : lib_frg_grid {
  void pscommon() {
    vec3 normal     = vec3(0, 1, 0);
    float mtl       = 0.0;
    float ruf       = 1.0;
    vec3 grid_color = grid_output2(frg_pos.xyz).xyz;
    out_gbuf        = packGbuffer(
        vec3(0.1),  // basecolor
        grid_color, // emmisive
        normal,     // normal
        ruf,
        mtl);
  }
}
fragment_shader ps_gbuffer_nx : iface_fgrid_gbuffer : lib_frgdeferred_grid_pbr {
  pscommon();
}
fragment_shader ps_gbuffer_nx_stereo : iface_fgrid_gbuffer : lib_frgdeferred_grid_pbr {
  pscommon();
}
///////////////////////////////////////////////////////////////
fragment_shader ps_grid_deferred_lit_mono_v4 
 : iface_fgrid_gbuffer 
 : lib_frgdeferred_grid_pbr 
 : lib_frg_grid {
  vec4 grid_color = grid_output4(frg_pos.xyz);
  vec3 normal     = vec3(0, 1, 0);
  float mtl       = 0.0;
  float ruf       = 1.0;
  out_gbuf        = packGbuffer(
      grid_color.xyz,  // basecolor
      vec3(0,0,0), // emmisive
      normal,     // normal
      ruf,
      mtl);

}
fragment_shader ps_grid_deferred_lit_mono_v6
 : iface_fgrid_gbuffer 
 : lib_frgdeferred_grid_pbr 
 : lib_frg_grid {
  vec3 C = texture(CNMREA,vec3(frg_uv0,0)).xyz;
  vec3 MR = texture(CNMREA,vec3(frg_uv0,2)).xyz;
  vec3 grid_color = C*ModColor.xyz*BaseColor.xyz;
  vec3 normal     = vec3(0, 1, 0);
  float mtl       = MR.z;
  float ruf       = MR.y;
  out_gbuf        = packGbuffer(
      grid_color.xyz,  // basecolor
      vec3(0,0,0), // emmisive
      normal,     // normal
      ruf,
      mtl);

}
